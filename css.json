{
	// Place your snippets for css here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Supports Grid": {
		"prefix": "Supports Grid",
		"body": [
			"@supports (grid-area: auto) {",
			"\t$0",
			"}"
		],
		"description": "Log output to console"
	},
	"Media Query": {
		"prefix": "Max Width",
		"body": [
			"@media screen and (max-width: $1px) {",
			"\t$0",
			"}"
		],
		"description": "A Basic Max-width Media Query"
	},
	"Background Defaults": {
		"prefix": "Background Defaults",
		"body": [
			"background-position: center;",
			"background-size: cover;",
			"background-repeat: no-repeat;"
		],
		"description": "A Basic CSS Background"
	},
	"Box Shadow": {
		"prefix": "Box Shadow",
		"body": [
			"box-shadow: 0px 2px 3px 0px #000000;"
		],
		"description": "A Simple Box Shadow Template"
	},
	"Object Fit": {
		"prefix": "object fit",
		"body": [
			"object-fit: cover;",
			"object-position: center;",
			"height: 100%;",
			"width: 100%;"
		],
		"description": "Standard object fit settings"
	},
	"Small Media Query": {
		"prefix": "@media-sm",
		"body": [
			"@media screen and (max-width: 559.98px){",
			"\t$0",
			"}",
		],
		"description": "Small Boostrtap Media query"
	},
	"Medium Media Query": {
		"prefix": "@media-md",
		"body": [
			"@media screen and (max-width: 767.98px){",
			"\t$0",
			"}",
		],
		"description": "Medium Boostrtap Media query"
	},
	"Large Media Query": {
		"prefix": "@media-lg",
		"body": [
			"@media screen and (max-width: 991.98px){",
			"\t$0",
			"}",
		],
		"description": "large Boostrtap Media query"
	},
	"XL Media Query": {
		"prefix": "@media-xl",
		"body": [
			"@media screen and (max-width: 1199.98px){",
			"\t$0",
			"}",
		],
		"description": "Extra Large Boostrtap Media query"
	},
	"XXL Media Query": {
		"prefix": "@media-xxl",
		"body": [
			"@media screen and (max-width: 1399.98px){",
			"\t$0",
			"}",
		],
		"description": "2X Larage Boostrtap Media query"
	}
}
